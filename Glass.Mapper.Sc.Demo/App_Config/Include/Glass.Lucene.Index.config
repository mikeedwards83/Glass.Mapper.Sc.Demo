<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<contentSearch>
			<configuration type="Sitecore.ContentSearch.LuceneProvider.LuceneSearchConfiguration, Sitecore.ContentSearch.LuceneProvider">
				<defaultIndexConfiguration type="Sitecore.ContentSearch.LuceneProvider.LuceneIndexConfiguration, Sitecore.ContentSearch.LuceneProvider">
					<fieldMap             type="Sitecore.ContentSearch.FieldMap, Sitecore.ContentSearch">
						<fieldNames hint="raw:AddFieldByFieldName">
							<field fieldName="IPageTitlesLongTitle" storageType="YES"  indexType="TOKENIZED"    vectorType="NO" boost="1f" type="System.String" settingType="Sitecore.ContentSearch.LuceneProvider.LuceneSearchFieldConfiguration, Sitecore.ContentSearch.LuceneProvider" />
						</fieldNames>
					</fieldMap>
				</defaultIndexConfiguration>
			</configuration>
		</contentSearch>
	</sitecore>
</configuration>